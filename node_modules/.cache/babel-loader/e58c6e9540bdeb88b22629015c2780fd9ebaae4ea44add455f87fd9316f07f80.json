ked = true\r\n              this.allChecked.fill(true)\r\n         }else{\r\n            this.headerChecked = false\r\n              this.allChecked.fill(false)\r\n         }\r\n        }, \r\n        handleScroll(){\r\n            const scrollTop = document.body.scrollTop\r\n      const clientHeight = document.body.clientHeight\r\n      const scrollHeight = document.body.scrollHeight\r\n    \r\n            console.log(scrollTop + clientHeight + scrollHeight)\r\n        }\r\n\r\n    },\r\n    \r\n    created(){  \r\n    this.loadAyantdroits()\r\n    document.body.addEventListener('scroll', this.handleScroll)\r\n    }\r\n    \r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.svg {\r\n    margin-left: 10px;\r\n}\r\n * {\r\n    box-sizing: border-box;\r\n    margin: 0;\r\n    padding: 0;\r\n  }\r\n \r\n  .container {\r\n    margin: 30px 0;\r\n    margin-right: auto;\r\n   \r\n    border: 1px solid steelblue;\r\n    padding: 30px;\r\n    border-radius: 5px;\r\n    background-color: #F2F4F7;\r\n  }\r\n\r\n\r\n.header-cell{\r\n    \r\n    padding: 10px 40px;\r\n    font-family: Inter, sans-serif;\r\n    font-size: 9px;\r\n    font-weight: 400;\r\n    text-transform: uppercase;\r\n    text-align: left;\r\n   \r\n}\r\n\r\n\r\n.cell {\r\n  \r\n    padding: 15px 40px;\r\n    font-family: Inter, sans-serif;\r\n    font-size: 12px;\r\n    font-weight: 600;\r\n    background-color: white;\r\n    text-align: left;\r\n    \r\n}\r\n\r\ntr:hover td{\r\n    background-color: #32b2e3;    /*#ebeefc */\r\n    cursor: pointer;\r\n}\r\n\r\n\r\ntable {\r\n    \r\n    border-spacing: 0 3px;\r\n    margin-right: 20px;\r\n    margin-top: 40px;\r\n    margin-left: auto;\r\n\r\n\r\n\r\n}\r\n\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}